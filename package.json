{
  "name": "@ladjs/api",
  "description": "API server for Lad",
  "version": "1.1.7",
  "author": "Nick Baugh <niftylettuce@gmail.com> (http://niftylettuce.com/)",
  "ava": {
    "failFast": true,
    "verbose": true
  },
  "bugs": {
    "url": "https://github.com/ladjs/api/issues",
    "email": "niftylettuce@gmail.com"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "contributors": [
    "Nick Baugh <niftylettuce@gmail.com> (http://niftylettuce.com/)"
  ],
  "dependencies": {
    "@koa/router": "^9.0.1",
    "@ladjs/i18n": "^3.0.12",
    "@ladjs/redis": "^1.0.4",
    "@ladjs/shared-config": "^3.0.6",
    "@ladjs/store-ip-address": "^0.0.7",
    "boolean": "3.0.1",
    "cabin": "^6.1.4",
    "express-request-id": "^1.4.1",
    "kcors": "^2.2.2",
    "koa": "^2.12.0",
    "koa-404-handler": "^0.0.2",
    "koa-basic-auth": "^4.0.0",
    "koa-better-error-handler": "^4.0.3",
    "koa-better-timeout": "^0.0.4",
    "koa-bodyparser": "^4.3.0",
    "koa-compress": "^4.0.1",
    "koa-conditional-get": "^2.0.0",
    "koa-connect": "^2.0.1",
    "koa-etag": "^3.0.0",
    "koa-helmet": "^5.2.0",
    "koa-json": "^2.0.2",
    "koa-no-trailing-slash": "^2.1.0",
    "koa-simple-ratelimit": "^4.0.2",
    "lodash": "^4.17.15",
    "request-received": "^0.0.3",
    "response-time": "^2.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "ava": "^3.8.2",
    "codecov": "^3.7.0",
    "cross-env": "^7.0.2",
    "eslint": "6.x",
    "eslint-config-xo-lass": "^1.0.3",
    "fixpack": "^3.0.6",
    "husky": "^4.2.5",
    "lint-staged": "10.2.7",
    "mongoose": "^5.9.16",
    "nyc": "^15.0.1",
    "remark-cli": "^8.0.0",
    "remark-preset-github": "^1.0.1",
    "supertest": "^4.0.2",
    "xo": "0.25"
  },
  "engines": {
    "node": ">=8.3"
  },
  "files": [
    "index.js"
  ],
  "homepage": "https://github.com/ladjs/api",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "api",
    "error",
    "express",
    "fast",
    "handling",
    "json",
    "koa",
    "lad",
    "lass",
    "light",
    "lightweight",
    "limit",
    "limiting",
    "modern",
    "rate",
    "ratelimit",
    "ratelimiting",
    "response",
    "server",
    "simple",
    "stripe",
    "web"
  ],
  "license": "MIT",
  "main": "index.js",
  "prettier": {
    "singleQuote": true,
    "bracketSpacing": true,
    "trailingComma": "none"
  },
  "remarkConfig": {
    "plugins": [
      "preset-github"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ladjs/api"
  },
  "scripts": {
    "ava": "cross-env NODE_ENV=test ava",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "xo && remark . -qfo",
    "nyc": "cross-env NODE_ENV=test nyc ava",
    "test": "npm run lint && npm run ava",
    "test-coverage": "npm run lint && npm run nyc"
  },
  "xo": {
    "prettier": true,
    "space": true,
    "extends": [
      "xo-lass"
    ],
    "ignores": [
      "config.js"
    ]
  }
}
